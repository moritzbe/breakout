
<h1>Update Positions!</h1>
	  <script type="text/javascript"
      src="http://maps.googleapis.com/maps/api/js?libraries=geometry&key=AIzaSyAq5jqy6DxgQBkk4KoTPgqEk2Pcwc0WfwE&sensor=False"></script>

<button id="getLocation" type="submit">Update Position</button>


<div id="latitude"></div>
<div id="longitude"></div>
<div id="distance"></div>
<div id="address"></div>



<%= form_for ([@team, @position]) do |f| %>

	   <%=f.label :latitude %>
	   <%=f.text_field :latitude, :class => "latitude" %>
	   <br>
	   <%=f.label :longitude%>
	   <%=f.text_field :longitude, :class => "longitude"  %>
	   <br>
	   <%=f.label :distance %>
	   <%=f.text_field :distance %>
	   <br>
	   <%=f.label :text %>
	   <%=f.text_field :text %>
	   <br>
	<%= f.submit "Send" %>   
<% end %>
